#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatapp/node_modules/.pnpm/jake@10.8.7/node_modules/jake/bin/node_modules:/home/chatapp/node_modules/.pnpm/jake@10.8.7/node_modules/jake/node_modules:/home/chatapp/node_modules/.pnpm/jake@10.8.7/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules:/home/chatapp/node_modules/jake/bin/node_modules:/home/chatapp/node_modules/jake/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/chatapp/node_modules/.pnpm/jake@10.8.7/node_modules/jake/bin/node_modules:/home/chatapp/node_modules/.pnpm/jake@10.8.7/node_modules/jake/node_modules:/home/chatapp/node_modules/.pnpm/jake@10.8.7/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules:/home/chatapp/node_modules/jake/bin/node_modules:/home/chatapp/node_modules/jake/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jake/bin/cli.js" "$@"
else
  exec node  "$basedir/../jake/bin/cli.js" "$@"
fi
