#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/esm/node_modules:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/node_modules:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules/glob/node_modules:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/esm/node_modules:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules/glob/dist/node_modules:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules/glob/node_modules:/home/chatapp/node_modules/.pnpm/glob@10.3.12/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../dist/esm/bin.mjs" "$@"
fi
