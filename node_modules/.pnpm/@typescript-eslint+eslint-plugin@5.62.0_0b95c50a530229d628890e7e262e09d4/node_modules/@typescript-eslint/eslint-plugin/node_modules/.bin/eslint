#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatapp/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/bin/node_modules:/home/chatapp/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/node_modules:/home/chatapp/node_modules/.pnpm/eslint@8.57.0/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules:/home/chatapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_0b95c50a530229d628890e7e262e09d4/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/chatapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_0b95c50a530229d628890e7e262e09d4/node_modules/@typescript-eslint/node_modules:/home/chatapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_0b95c50a530229d628890e7e262e09d4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/chatapp/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/bin/node_modules:/home/chatapp/node_modules/.pnpm/eslint@8.57.0/node_modules/eslint/node_modules:/home/chatapp/node_modules/.pnpm/eslint@8.57.0/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules:/home/chatapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_0b95c50a530229d628890e7e262e09d4/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/chatapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_0b95c50a530229d628890e7e262e09d4/node_modules/@typescript-eslint/node_modules:/home/chatapp/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.62.0_0b95c50a530229d628890e7e262e09d4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.57.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.57.0/node_modules/eslint/bin/eslint.js" "$@"
fi
