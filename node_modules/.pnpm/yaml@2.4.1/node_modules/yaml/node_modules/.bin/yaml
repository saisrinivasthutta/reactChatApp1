#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatapp/node_modules/.pnpm/yaml@2.4.1/node_modules/yaml/node_modules:/home/chatapp/node_modules/.pnpm/yaml@2.4.1/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/chatapp/node_modules/.pnpm/yaml@2.4.1/node_modules/yaml/node_modules:/home/chatapp/node_modules/.pnpm/yaml@2.4.1/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.mjs" "$@"
else
  exec node  "$basedir/../../bin.mjs" "$@"
fi
