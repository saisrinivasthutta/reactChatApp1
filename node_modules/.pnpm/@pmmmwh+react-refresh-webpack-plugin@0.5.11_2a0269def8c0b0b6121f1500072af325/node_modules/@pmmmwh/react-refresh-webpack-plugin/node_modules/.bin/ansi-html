#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatapp/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/home/chatapp/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/home/chatapp/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules:/home/chatapp/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.11_2a0269def8c0b0b6121f1500072af325/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/home/chatapp/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.11_2a0269def8c0b0b6121f1500072af325/node_modules/@pmmmwh/node_modules:/home/chatapp/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.11_2a0269def8c0b0b6121f1500072af325/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/chatapp/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/home/chatapp/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/home/chatapp/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/home/chatapp/node_modules/.pnpm/node_modules:/home/chatapp/node_modules:/home/node_modules:/node_modules:/home/chatapp/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.11_2a0269def8c0b0b6121f1500072af325/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/home/chatapp/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.11_2a0269def8c0b0b6121f1500072af325/node_modules/@pmmmwh/node_modules:/home/chatapp/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.11_2a0269def8c0b0b6121f1500072af325/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
else
  exec node  "$basedir/../../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
fi
